{
  "name": "nva-frontend",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint . --ext .ts,.tsx",
    "pre-commit": "npx lint-staged && npm run unused-i18n-keys",
    "prepare": "husky",
    "preview": "vite preview",
    "start": "vite",
    "start:mock": "set VITE_USE_MOCK=true&& npm start",
    "start:mock:win": "set VITE_USE_MOCK=true&& npm start",
    "start:mock:unix": "export VITE_USE_MOCK=true && npm start",
    "test": "cypress run",
    "test:vitest": "vitest",
    "test:vitest:verbose": "vitest run --reporter=verbose",
    "test:cypress": "start-server-and-test start:mock:win http://localhost:3000 test",
    "unused-i18n-keys": "i18n-unused display-unused"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@fontsource/roboto": "^5.1.0",
    "@mui/icons-material": "^6.1.10",
    "@mui/lab": "^6.0.0-beta.18",
    "@mui/material": "^6.1.9",
    "@mui/x-date-pickers": "^7.23.0",
    "@reduxjs/toolkit": "^2.4.0",
    "@tanstack/react-query": "^5.62.2",
    "@uppy/aws-s3": "^4.1.2",
    "@uppy/core": "^4.2.3",
    "@uppy/dashboard": "^4.1.2",
    "@uppy/drag-drop": "^4.0.4",
    "@uppy/file-input": "^4.0.4",
    "@uppy/locales": "^4.3.0",
    "@uppy/progress-bar": "^4.0.1",
    "@uppy/react": "^4.0.3",
    "aws-amplify": "^6.10.2",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "deepmerge": "^4.3.1",
    "formik": "^2.4.6",
    "i18n-iso-countries": "^7.13.0",
    "i18next": "^24.0.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "isbn3": "^1.2.4",
    "nva-language": "^1.0.19",
    "pretty-bytes": "^6.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.1.3",
    "react-imask": "^7.6.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^5.3.4",
    "yup": "^1.5.0"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.62.1",
    "@tanstack/react-query-devtools": "^5.62.2",
    "@types/isbn-utils": "^1.1.6",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.13",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "axe-core": "^4.10.2",
    "axios-mock-adapter": "^2.1.0",
    "cypress": "^13.16.1",
    "cypress-axe": "^1.5.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "husky": "^9.1.7",
    "i18n-unused": "^0.17.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.2",
    "start-server-and-test": "^2.0.8",
    "typescript": "^5.7.2",
    "vite": "^6.0.2",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^2.1.8"
  }
}
