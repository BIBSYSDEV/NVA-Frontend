{
  "name": "nva-frontend",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint . --ext .ts,.tsx",
    "pre-commit": "npx lint-staged && npm run unused-i18n-keys",
    "prepare": "husky",
    "preview": "vite preview",
    "start": "vite",
    "start:mock": "set VITE_USE_MOCK=true&& npm start",
    "start:mock:win": "set VITE_USE_MOCK=true&& npm start",
    "start:mock:unix": "export VITE_USE_MOCK=true && npm start",
    "test": "cypress run",
    "test:vitest": "vitest",
    "test:vitest:verbose": "vitest run --reporter=verbose",
    "test:cypress": "start-server-and-test start:mock:win http://localhost:3000 test",
    "unused-i18n-keys": "i18n-unused display-unused"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.9",
    "@mui/icons-material": "^7.3.5",
    "@mui/lab": "^7.0.0-beta.17",
    "@mui/material": "^7.1.1",
    "@mui/utils": "^7.1.1",
    "@mui/x-date-pickers": "^8.17.0",
    "@reduxjs/toolkit": "^2.11.2",
    "@tanstack/react-query": "^5.90.12",
    "@uppy/aws-s3": "^5.1.0",
    "@uppy/core": "^5.1.0",
    "@uppy/dashboard": "^5.1.0",
    "@uppy/locales": "^5.1.0",
    "@uppy/react": "^5.1.1",
    "aws-amplify": "^6.15.9",
    "axios": "^1.13.2",
    "bibtex-generator": "^1.0.11",
    "date-fns": "^4.1.0",
    "deepmerge": "^4.3.1",
    "formik": "^2.4.9",
    "i18n-iso-countries": "^7.14.0",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "isbn3": "^2.0.1",
    "nva-language": "^1.0.19",
    "pretty-bytes": "^7.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "react-i18next": "^16.5.0",
    "react-imask": "^7.6.1",
    "react-redux": "^9.2.0",
    "react-router": "^7.11.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "axe-core": "^4.11.0",
    "axios-mock-adapter": "^2.1.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "cypress": "^15.8.1",
    "cypress-axe": "^1.7.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "i18n-unused": "^0.19.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.7",
    "prettier": "^3.7.4",
    "start-server-and-test": "^2.1.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vite-plugin-checker": "^0.12.0",
    "vitest": "^3.2.4"
  }
}
