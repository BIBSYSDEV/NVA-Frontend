    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.5",
    "i18n-unused": "^0.16.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.5",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-eslint": "^1.8.1"
  }
