  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "test:cypress": "REACT_APP_USE_MOCK=true start-server-and-test http://localhost:3000",
    "test:react": "react-scripts test --coverage --watchAll=false",
    "test": "cypress run",
    "deploy": "aws s3 sync build/ s3://demo-stack-bucket",
    "eject": "react-scripts eject",
    "generateTestFiles": "sh generateTestFiles.sh"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true
  },
  "devDependencies": {
    "@cypress/code-coverage": "^1.10.2",
    "@cypress/instrument-cra": "^1.0.0",
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/classnames": "^2.2.9",
    "@types/fetch-mock": "^7.3.2",
    "@types/jest": "24.0.18",
    "@types/material-ui": "^0.21.7",
    "@types/node": "12.7.5",
    "@types/node-sass": "^4.11.0",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^4.3.5",
    "@types/styled-components": "^4.4.0",
    "@types/uuid": "^3.4.6",
    "@types/yup": "^0.26.26",
    "@typescript-eslint/parser": "^2.9.0",
    "axios-mock-adapter": "^1.17.0",
    "eslint-plugin-prettier": "^3.1.0",
    "faker": "^4.1.0",
    "fetch-mock": "^7.7.3",
    "fs": "0.0.1-security",
    "husky": "^3.1.0",
    "istanbul-lib-coverage": "^2.0.5",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "start-server-and-test": "^1.10.6",
    "ts-node": "^8.5.3",
    "typescript": "^3.7.2"
  }
