    "cypress": "^3.7.0",
    "date-fns": "^2.8.1",
    "formik": "^2.0.6",
    "formik-material-ui": "1.0.0",
    "i18next": "^19.0.1",
    "i18next-browser-languagedetector": "^4.0.1",
    "material-ui-flat-pagination": "^4.0.0",
