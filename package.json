{
  "name": "nva-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.4.2",
    "@material-ui/icons": "^4.4.1",
    "@types/react-router": "^5.1.1",
    "aws-amplify": "^1.1.40",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "cypress": "^3.4.1",
    "formik": "^1.5.8",
    "formik-material-ui": "0.0.22",
    "i18next": "^17.0.16",
    "i18next-browser-languagedetector": "^3.0.3",
    "material-ui-flat-pagination": "^4.0.0",
    "node-sass": "^4.12.0",
    "notistack": "^0.9.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-i18next": "^10.12.5",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.1.2",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "ts-loader": "^6.2.0",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "test:cypress": "REACT_APP_USE_MOCK=false start-server-and-test http://localhost:3000",
    "test": "cypress run",
    "deploy": "aws s3 sync build/ s3://demo-stack-bucket",
    "eject": "react-scripts eject",
    "generateTestFiles": "sh generateTestFiles.sh"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@cypress/code-coverage": "^1.10.1",
    "@cypress/instrument-cra": "^1.0.0",
    "@types/classnames": "^2.2.9",
    "@types/fetch-mock": "^7.3.1",
    "@types/jest": "24.0.18",
    "@types/material-ui": "^0.21.7",
    "@types/node": "12.7.5",
    "@types/node-sass": "^4.11.0",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^4.3.5",
    "@types/yup": "^0.26.24",
    "@typescript-eslint/parser": "^2.3.1",
    "eslint-plugin-prettier": "^3.1.0",
    "faker": "^4.1.0",
    "fetch-mock": "^7.3.9",
    "fs": "0.0.1-security",
    "husky": "^3.0.5",
    "istanbul-lib-coverage": "^2.0.5",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "start-server-and-test": "^1.10.4",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  }
}
