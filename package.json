{
  "name": "nva-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "pre-commit": "npx lint-staged && npm run unused-i18n-keys",
    "prepare": "husky",
    "start": "react-scripts -r @cypress/instrument-cra start",
    "start:mock:win": "set REACT_APP_USE_MOCK=true&& npm start",
    "start:mock:unix": "export REACT_APP_USE_MOCK=true && npm start",
    "test": "cypress run",
    "test:cypress": "start-server-and-test start:mock:win http://localhost:3000 test",
    "unused-i18n-keys": "i18n-unused display-unused"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.15.16",
    "@mui/lab": "5.0.0-alpha.170",
    "@mui/material": "^5.15.16",
    "@mui/x-date-pickers": "^7.3.2",
    "@reduxjs/toolkit": "^2.2.3",
    "@tanstack/react-query": "^5.35.1",
    "@uppy/aws-s3": "^3.6.2",
    "@uppy/core": "^3.11.1",
    "@uppy/dashboard": "^3.8.2",
    "@uppy/locales": "^3.5.3",
    "@uppy/react": "^3.2.2",
    "aws-amplify": "^6.3.0",
    "axios": "^1.6.8",
    "date-fns": "^3.6.0",
    "deepmerge": "^4.3.1",
    "formik": "^2.4.6",
    "i18n-iso-countries": "^7.11.1",
    "i18next": "^22.5.1",
    "i18next-browser-languagedetector": "^7.2.1",
    "isbn3": "^1.1.48",
    "nva-language": "^1.0.16",
    "pretty-bytes": "^6.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^12.3.1",
    "react-imask": "^7.6.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^5.3.4",
    "react-share": "^4.4.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.39",
    "@cypress/instrument-cra": "^1.4.0",
    "@tanstack/eslint-plugin-query": "^5.32.1",
    "@tanstack/react-query-devtools": "^5.35.1",
    "@types/isbn-utils": "^1.1.6",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "axe-core": "^4.9.1",
    "axios-mock-adapter": "^1.22.0",
    "cypress": "^13.9.0",
    "cypress-axe": "^1.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.2.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "i18n-unused": "^0.16.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "react-scripts": "^5.0.1",
    "start-server-and-test": "^2.0.3",
    "typescript": "^4.9.5"
  }
