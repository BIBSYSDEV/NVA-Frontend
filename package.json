{
  "name": "nva-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "pre-commit": "npx pretty-quick --staged && npm run unused-i18n-keys",
    "prepare": "husky install",
    "start": "react-scripts -r @cypress/instrument-cra start",
    "start:mock": "set REACT_APP_USE_MOCK=true&& npm start",
    "test": "cypress run",
    "test:parallel": "cypress run --parallel --record --ci-build-id $CODEBUILD_INITIATOR",
    "test:cypress": "start-server-and-test start:mock http://localhost:3000 test:parallel",
    "test:react": "react-scripts test --coverage --watchAll=false",
    "unused-i18n-keys": "i18n-unused display-unused"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript"
  },
  "dependencies": {
    "@aws-amplify/auth": "^4.6.4",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fontsource/barlow": "^4.5.8",
    "@fontsource/crimson-text": "^4.5.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/lab": "5.0.0-alpha.97",
    "@mui/material": "^5.10.3",
    "@mui/x-date-pickers": "^5.0.0-beta.7",
    "@reduxjs/toolkit": "^1.8.5",
    "@uppy/aws-s3-multipart": "^1.8.18",
    "@uppy/core": "^1.20.1",
    "@uppy/dashboard": "^1.21.1",
    "@uppy/locales": "^1.22.0",
    "@uppy/react": "^1.12.2",
    "axios": "^0.27.2",
    "date-fns": "^2.29.2",
    "deepmerge": "^4.2.2",
    "formik": "^2.2.9",
    "i18n-iso-countries": "^7.5.0",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "isbn-utils": "^1.1.0",
    "nva-language": "^1.0.11",
    "pretty-bytes": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^11.18.5",
    "react-imask": "^6.4.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^5.3.3",
    "react-share": "^4.4.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/instrument-cra": "^1.4.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/isbn-utils": "^1.1.3",
    "@types/jest": "^29.0.0",
    "@types/react": "^17.0.49",
    "@types/react-dom": "^17.0.17",
    "@types/react-router-dom": "^5.3.3",
    "axe-core": "^4.4.3",
    "axios-mock-adapter": "^1.21.2",
    "cypress": "^10.7.0",
    "cypress-axe": "^1.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "i18n-unused": "^0.10.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react-scripts": "^5.0.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.8.2"
  }
}
