{
  "name": "nva-frontend",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "pre-commit": "npx lint-staged && npm run unused-i18n-keys",
    "prepare": "husky",
    "preview": "vite preview",
    "start": "vite",
    "start:mock": "set VITE_USE_MOCK=true&& npm start",
    "start:mock:win": "set VITE_USE_MOCK=true&& npm start",
    "start:mock:unix": "export VITE_USE_MOCK=true && npm start",
    "test": "cypress run",
    "test:cypress": "start-server-and-test start:mock:win http://localhost:3000 test",
    "unused-i18n-keys": "i18n-unused display-unused"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.0.14",
    "@mui/icons-material": "^5.16.7",
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^5.16.7",
    "@mui/x-date-pickers": "^7.13.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@tanstack/react-query": "^5.51.24",
    "@uppy/aws-s3": "^3.6.2",
    "@uppy/core": "^3.13.0",
    "@uppy/dashboard": "^3.9.1",
    "@uppy/drag-drop": "^3.1.1",
    "@uppy/file-input": "^3.1.2",
    "@uppy/locales": "^3.5.4",
    "@uppy/progress-bar": "^3.1.1",
    "@uppy/react": "^3.4.0",
    "aws-amplify": "^6.5.1",
    "axios": "^1.7.4",
    "date-fns": "^3.6.0",
    "deepmerge": "^4.3.1",
    "formik": "^2.4.6",
    "i18n-iso-countries": "^7.11.3",
    "i18next": "^23.14.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "isbn3": "^1.1.51",
    "nva-language": "^1.0.19",
    "pretty-bytes": "^6.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.0.1",
    "react-imask": "^7.6.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^5.3.4",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.45",
    "@tanstack/eslint-plugin-query": "^5.52.0",
    "@tanstack/react-query-devtools": "^5.52.0",
    "@types/isbn-utils": "^1.1.6",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "axe-core": "^4.10.0",
    "axios-mock-adapter": "^2.0.0",
    "cypress": "^13.13.3",
    "cypress-axe": "^1.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
