{
  "name": "nva-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "pre-commit": "npx pretty-quick --staged && npm run unused-i18n-keys",
    "prepare": "husky install",
    "start": "react-scripts -r @cypress/instrument-cra start",
    "start:mock": "set REACT_APP_USE_MOCK=true&& npm start",
    "test": "cypress run",
    "test:cypress": "start-server-and-test start:mock http://localhost:3000 test",
    "unused-i18n-keys": "i18n-unused display-unused"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@aws-amplify/auth": "^5.3.6",
