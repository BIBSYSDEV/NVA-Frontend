    "@material-ui/styles": "^4.10.0",
    "@uppy/aws-s3-multipart": "^1.8.6",
    "@uppy/core": "^1.13.2",
    "@uppy/dashboard": "^1.12.6",
    "@uppy/react": "^1.10.6",
    "aws-amplify": "^3.1.1",
    "axios": "^0.19.2",
    "date-fns": "^2.16.1",
    "deepmerge": "^4.2.2",
    "formik": "^2.1.5",
    "i18next": "^19.7.0",
    "i18next-browser-languagedetector": "^6.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.2",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "react-sortable-hoc": "^1.11.0",
    "react-truncate": "^2.4.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.0",
    "yup": "^0.29.3"
  },
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "test:cypress": "REACT_APP_USE_MOCK=true start-server-and-test http://localhost:3000",
    "test:react": "react-scripts test --coverage --watchAll=false",
    "test": "cypress run",
    "deploy": "aws s3 sync build/ s3://demo-stack-bucket",
    "eject": "react-scripts eject",
    "generateTestFiles": "sh generateTestFiles.sh"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.8.1",
    "@cypress/instrument-cra": "^1.3.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/history": "^4.7.7",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-truncate": "^2.3.3",
    "@types/styled-components": "^5.1.3",
    "@types/yup": "^0.29.7",
    "axios-mock-adapter": "^1.18.2",
    "cypress": "^5.1.0",
    "faker": "^5.1.0",
    "husky": "^4.3.0",
    "nyc": "^15.1.0",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.2",
    "start-server-and-test": "^1.11.3",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7"
  }
}
