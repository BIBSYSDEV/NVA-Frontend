{
  "name": "nva-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "pre-commit": "npx lint-staged && npm run unused-i18n-keys",
    "prepare": "husky install",
    "start": "react-scripts -r @cypress/instrument-cra start",
    "start:mock": "set REACT_APP_USE_MOCK=true&& npm start",
    "test": "cypress run",
    "test:cypress": "start-server-and-test start:mock http://localhost:3000 test",
    "unused-i18n-keys": "i18n-unused display-unused"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@aws-amplify/auth": "^5.6.6",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.18",
    "@mui/lab": "5.0.0-alpha.153",
    "@mui/material": "^5.14.18",
    "@mui/x-date-pickers": "^6.18.1",
    "@reduxjs/toolkit": "^1.9.7",
    "@tanstack/react-query": "^4.35.7",
    "@uppy/aws-s3-multipart": "^3.9.0",
    "@uppy/core": "^3.7.1",
    "@uppy/dashboard": "^3.7.1",
    "@uppy/locales": "^3.4.0",
    "@uppy/react": "^3.1.3",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "deepmerge": "^4.3.1",
    "formik": "^2.4.5",
    "i18n-iso-countries": "^7.7.0",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "isbn-utils": "^1.1.0",
    "nva-language": "^1.0.15",
    "pretty-bytes": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.1",
    "react-i18next": "^12.3.1",
    "react-imask": "^7.1.3",
    "react-redux": "^8.1.3",
    "react-router-dom": "^5.3.4",
    "react-share": "^4.4.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.11",
    "@cypress/instrument-cra": "^1.4.0",
    "@tanstack/eslint-plugin-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@types/isbn-utils": "^1.1.6",
    "@types/jest": "^29.5.9",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-router-dom": "^5.3.3",
    "axe-core": "^4.8.2",
    "axios-mock-adapter": "^1.22.0",
    "cypress": "^13.5.1",
    "cypress-axe": "^1.5.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "i18n-unused": "^0.16.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
