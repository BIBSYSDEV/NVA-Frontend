{
  "name": "nva-frontend",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint . --ext .ts,.tsx",
    "pre-commit": "npx lint-staged && npm run unused-i18n-keys",
    "prepare": "husky",
    "preview": "vite preview",
    "start": "vite",
    "start:mock": "set VITE_USE_MOCK=true&& npm start",
    "start:mock:win": "set VITE_USE_MOCK=true&& npm start",
    "start:mock:unix": "export VITE_USE_MOCK=true && npm start",
    "test": "cypress run",
    "test:vitest": "vitest",
    "test:vitest:verbose": "vitest run --reporter=verbose",
    "test:cypress": "start-server-and-test start:mock:win http://localhost:3000 test",
    "unused-i18n-keys": "i18n-unused display-unused"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.5",
    "@mui/icons-material": "^6.4.7",
    "@mui/lab": "^6.0.0-beta.30",
    "@mui/material": "^6.4.3",
    "@mui/utils": "^6.4.3",
    "@mui/x-date-pickers": "^7.27.3",
    "@reduxjs/toolkit": "^2.6.1",
    "@tanstack/react-query": "^5.68.0",
    "@uppy/aws-s3": "^4.2.3",
    "@uppy/core": "^4.4.3",
    "@uppy/dashboard": "^4.3.2",
    "@uppy/drag-drop": "^4.1.1",
    "@uppy/file-input": "^4.1.1",
    "@uppy/locales": "^4.5.1",
    "@uppy/progress-bar": "^4.2.1",
    "@uppy/react": "^4.2.2",
    "aws-amplify": "^6.13.5",
    "axios": "^1.8.3",
    "date-fns": "^4.1.0",
    "deepmerge": "^4.3.1",
    "formik": "^2.4.6",
    "i18n-iso-countries": "^7.14.0",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "isbn3": "^1.2.8",
    "nva-language": "^1.0.19",
    "pretty-bytes": "^6.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.4.1",
    "react-imask": "^7.6.1",
    "react-redux": "^9.2.0",
    "react-router": "^7.3.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.68.0",
    "@tanstack/react-query-devtools": "^5.68.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.13",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "^4.3.4",
    "axe-core": "^4.10.3",
    "axios-mock-adapter": "^2.1.0",
    "cypress": "^14.2.0",
    "cypress-axe": "^1.6.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "i18n-unused": "^0.17.2",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.11",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^3.0.8"
  }
}
