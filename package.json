  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "test:cypress": "REACT_APP_USE_MOCK=true start-server-and-test http://localhost:3000",
    "test:react": "react-scripts test --coverage --watchAll=false",
    "test": "cypress run",
    "deploy": "aws s3 sync build/ s3://demo-stack-bucket",
    "eject": "react-scripts eject",
    "generateTestFiles": "sh generateTestFiles.sh"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true
  },
  "devDependencies": {
    "@cypress/code-coverage": "^1.10.4",
    "@cypress/instrument-cra": "^1.0.0",
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.24",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^4.3.5",
    "@types/styled-components": "^4.4.2",
    "@types/uuid": "^3.4.6",
    "@types/yup": "^0.26.27",
    "@typescript-eslint/parser": "^2.14.0",
    "amazon-cognito-identity-js": "^3.2.1",
    "axios-mock-adapter": "^1.17.0",
    "cypress": "^3.8.3",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "fs": "0.0.1-security",
    "history": "^4.10.1",
    "husky": "^3.1.0",
    "istanbul-lib-coverage": "^3.0.0",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "start-server-and-test": "^1.10.6",
    "ts-node": "^8.6.0",
    "typescript": "^3.7.4"
  }
