    "@tanstack/react-query-devtools": "^4.29.6",
    "@types/isbn-utils": "^1.1.3",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
    "axe-core": "^4.7.0",
    "axios-mock-adapter": "^1.21.4",
    "cypress": "^12.12.0",
    "cypress-axe": "^1.4.0",
